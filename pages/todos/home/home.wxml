    <view class="bg-white padding margin">
        <navigator url="{{article_url}}">
            <text class="cuIcon-notificationfill text-blue text-xl"></text>
            {{notice}}
        </navigator>
    </view>

    <view class="cu-bar bg-white solid-bottom margin-top margin-lr">
        <view class='action'>
            <text class='cuIcon-titles text-blue '></text> 学习在线
        </view>
    </view>
    <view class="cu-card case no-card margin-lr">
        <view class="cu-item shadow">
            <view class="cu-list grid col-{{gridCol}} ">
                <view class="cu-item" wx:for="{{iconList}}" wx:key="index" wx:if="{{index<gridCol*2}}">
                    <navigator hover-class='none' url="{{item.url}}">
                        <view class="cuIcon-{{item.icon}} text-{{item.color}}">
                        </view>
                        <text>{{item.name}}</text>
                    </navigator>
                </view>
            </view>
        </view>
    </view>

    <view class="cu-bar bg-white solid-bottom margin-top margin-lr">
        <view class='action'>
            <text class='cuIcon-titles text-blue '></text> 法规制度
        </view>
    </view>
    <view class="cu-card case no-card margin-bottom-xl margin-lr">
        <view class="cu-item shadow">
            <view class="cu-list grid col-{{gridCol}} ">
                <view class="cu-item" wx:for="{{lawList}}" wx:key="index" wx:if="{{index<gridCol*2}}">
                    <navigator hover-class='none' url="{{item.url}}">
                        <view class="cuIcon-{{item.icon}} text-{{item.color}}">
                        </view>
                        <text>{{item.name}}</text>
                    </navigator>
                </view>
            </view>
        </view>
    </view>

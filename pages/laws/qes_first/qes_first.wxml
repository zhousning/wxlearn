<cu-custom bgColor="bg-blue" isBack="{{true}}">
    <view slot="backText">返回</view>
    <view slot="content">{{header_title}}</view>
</cu-custom>

<scroll-view scroll-y class="scrollPage">
    <view class="padding text-left">
        <block wx:for="{{iconList}}" wx:key>
            <block wx:if="{{item.ctg == 'PDF'}}">
                <view data-url="{{item.attach}}" bindtap="openDoc" class="padding-xl radius shadow-warp bg-white margin-top">
                    <text class="text-black text-lg"> {{item.name}}</text>
                </view>
            </block>
            <block wx:else>
                <navigator open-type="navigate" hover-class='none' url="{{item.url}}" class="padding-xl radius shadow-warp bg-white margin-top">
                    <text class="text-black text-lg"> {{item.name}}</text>
                </navigator>
            </block>

        </block>
    </view>
    <view class='cu-tabbar-height'></view>
</scroll-view>